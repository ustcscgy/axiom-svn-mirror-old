  withval="$with_included_noweb"
  case $withval in
                yes) if test -f $axiom_top_srcdir/zips/noweb-2.10a.tgz ; then
                         axiom_build_noweb=yes
                     else
                         { { echo "$as_me:$LINENO: error: --with-included-noweb specified, but no noweb sources is present" >&5
echo "$as_me: error: --with-included-noweb specified, but no noweb sources is present" >&2;}
   { (exit 1); exit 1; }; }
                     fi  ;;
                no) axiom_build_noweb=no ;;
                *) { { echo "$as_me:$LINENO: error: erroneous value for --with-included-noweb" >&5
echo "$as_me: error: erroneous value for --with-included-noweb" >&2;}
   { (exit 1); exit 1; }; } ;;
             esac
fi;
## Check for notangle and noweb if we are not explicitly told
## to build noweb from Axiom sources.
if test x$axiom_build_noweb != xyes ; then
    for ac_prog in notangle
do
  # Extract the first word of "$ac_prog", so it can be a program name with args.
set dummy $ac_prog; ac_word=$2
echo "$as_me:$LINENO: checking for $ac_word" >&5
echo $ECHO_N "checking for $ac_word... $ECHO_C" >&6
if test "${ac_cv_prog_NOTANGLE+set}" = set; then
  echo $ECHO_N "(cached) $ECHO_C" >&6
else
  if test -n "$NOTANGLE"; then
  ac_cv_prog_NOTANGLE="$NOTANGLE" # Let the user override the test.
else
as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
  for ac_exec_ext in '' $ac_executable_extensions; do
  if $as_executable_p "$as_dir/$ac_word$ac_exec_ext"; then
    ac_cv_prog_NOTANGLE="$ac_prog"
    echo "$as_me:$LINENO: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
done

fi
fi
NOTANGLE=$ac_cv_prog_NOTANGLE
if test -n "$NOTANGLE"; then
  echo "$as_me:$LINENO: result: $NOTANGLE" >&5
echo "${ECHO_T}$NOTANGLE" >&6
else
  echo "$as_me:$LINENO: result: no" >&5
echo "${ECHO_T}no" >&6
fi

  test -n "$NOTANGLE" && break
done

    for ac_prog in noweave
do
  # Extract the first word of "$ac_prog", so it can be a program name with args.
set dummy $ac_prog; ac_word=$2
echo "$as_me:$LINENO: checking for $ac_word" >&5
echo $ECHO_N "checking for $ac_word... $ECHO_C" >&6
if test "${ac_cv_prog_NOWEAVE+set}" = set; then
  echo $ECHO_N "(cached) $ECHO_C" >&6
else
  if test -n "$NOWEAVE"; then
  ac_cv_prog_NOWEAVE="$NOWEAVE" # Let the user override the test.
else
as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
  for ac_exec_ext in '' $ac_executable_extensions; do
  if $as_executable_p "$as_dir/$ac_word$ac_exec_ext"; then
    ac_cv_prog_NOWEAVE="$ac_prog"
    echo "$as_me:$LINENO: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
done

fi
fi
NOWEAVE=$ac_cv_prog_NOWEAVE
if test -n "$NOWEAVE"; then
  echo "$as_me:$LINENO: result: $NOWEAVE" >&5
echo "${ECHO_T}$NOWEAVE" >&6
else
  echo "$as_me:$LINENO: result: no" >&5
echo "${ECHO_T}no" >&6
fi

  test -n "$NOWEAVE" && break
done


    ## In case noweb is missing we need to build our own.
    if test -z $NOTANGLE -o -z $NOWEAVE ; then
        if test x$axiom_build_noweb = xno ; then
           { { echo "$as_me:$LINENO: error: noweb utils are missing but --without-included-noweb is specified" >&5
echo "$as_me: error: noweb utils are missing but --without-included-noweb is specified" >&2;}
   { (exit 1); exit 1; }; }
        elif test -f $axiom_top_srcdir/zips/noweb-2.10a.tgz ; then
           axiom_build_noweb=yes
        else
           { { echo "$as_me:$LINENO: error: noweb utils and noweb sources missing" >&5
echo "$as_me: error: noweb utils and noweb sources missing" >&2;}
   { (exit 1); exit 1; }; }
        fi
    fi
