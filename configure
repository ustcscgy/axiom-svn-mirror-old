         axiom_lisp_flavor=gcl
         axiom_fasl_type=o
       else
          { { echo "$as_me:$LINENO: error: GCL and GCL sources missing, see README.wh" >&5
echo "$as_me: error: GCL and GCL sources missing, see README.wh" >&2;}
   { (exit 1); exit 1; }; }
       fi
       ;;
    yes,*)
       { { echo "$as_me:$LINENO: error: --with-lisp=$axiom_lisp conflicts with --enable-gcl" >&5
echo "$as_me: error: --with-lisp=$axiom_lisp conflicts with --enable-gcl" >&2;}
   { (exit 1); exit 1; }; }
       ;;
esac


# Check whether --with-lisp-flavor or --without-lisp-flavor was given.
