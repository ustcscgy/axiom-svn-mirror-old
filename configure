axiom_include_gcl=
if test -z $axiom_lisp; then
    # Check whether --enable-gcl or --disable-gcl was given.
if test "${enable_gcl+set}" = set; then
  enableval="$enable_gcl"
  case $withval in
                       yes|no) axiom_include_gcl=$withval ;;
                       *) { { echo "$as_me:$LINENO: error: erroneous value for --enable-gcl" >&5
echo "$as_me: error: erroneous value for --enable-gcl" >&2;}
   { (exit 1); exit 1; }; } ;;
                   esac
fi;
fi
## We need to build our own GCL is none is avalaible
if test -z $axiom_lisp; then
    # Extract the first word of "gcl", so it can be a program name with args.
