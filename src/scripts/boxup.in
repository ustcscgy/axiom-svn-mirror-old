boxhead=@axiom_build_sharedir@/typeset/boxhead
boxtail=@axiom_build_sharedir@/typeset/boxtail

cp $boxhead $1.pamphlet
cat $1 >>$1.pamphlet
cat $boxtail >>$1.pamphlet
mv $1 $1.org
@axiom_build_document@ $1
echo diffing $1 $1.org
diff $1 $1.org
rm -f $1.dvi
echo if files do not differ it is safe to rm $1 $1.org

