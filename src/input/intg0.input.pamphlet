--R     + 
--R         2 2 +-+    3+-+2 3+-------+
--R       4b x \|3 log(\|a   \|b x + a - a)
--R     + 
--R                  +-+3+-+2 3+-------+    +-+
--R        2 2     2\|3 \|a   \|b x + a + a\|3                  +-+3+-+3+-------+2
--R     12b x atan(----------------------------) + (12b x - 9a)\|3 \|a \|b x + a
--R                             3a
--R  /
--R        2 2 +-+3+-+
--R     18a x \|3 \|a
--R                                          Type: Union(Expression Integer,...)
--E 13

\end{chunk}
Examples of transcendentals over a curve of genus 0
\begin{chunk}{*}
--S 14 of 25
t6:=x / (y + y**2) + log(y + 1)
--R 
--R
--R           +-------+                +-------+
--R         (\|a x + b  + a x + b)log(\|a x + b  + 1) + x
--R   (14)  ---------------------------------------------
--R                       +-------+
--R                      \|a x + b  + a x + b
--R                                                     Type: Expression Integer
--E 14

--S 15 of 25
integrate(t6,x)
--R 
--R
--R   (15)
--R          2                            +-------+                 +-------+
--R       (2a x + (2a - 4)b - 2a + 4)log(\|a x + b  + 1) + (2a - 4)\|a x + b
--R     + 
--R           2
--R       (- a  + 2a)x
--R  /
--R       2
--R     2a
--R                                          Type: Union(Expression Integer,...)
--E 15

--S 16 of 25
t7:=(2 + 1/sqrt(x)) * cos(x + sqrt x)
--R 
--R
--R            +-+          +-+
--R         (2\|x  + 1)cos(\|x  + x)
--R   (16)  ------------------------
--R                    +-+
--R                   \|x
--R                                                     Type: Expression Integer
--E 16

--S 17 of 25
integrate(t7,x)
--R 
--R
--R               +-+
--R   (17)  2sin(\|x  + x)
--R                                          Type: Union(Expression Integer,...)
--E 17

\end{chunk}
