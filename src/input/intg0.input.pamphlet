Some genus 0 integrations
\begin{chunk}{*}
--S 1 of 25
y := sqrt(a * x + b)
--R 
--R
--R         +-------+
--R   (1)  \|a x + b
--R                                                     Type: Expression Integer
--E 1

--S 2 of 25
integrate(y,x)
--R 
--R
--R                    +-------+
--R        (2a x + 2b)\|a x + b
--R   (2)  ---------------------
--R                  3a
--R                                          Type: Union(Expression Integer,...)
--E 2

--S 3 of 25
t1:=x * y
--R 
--R
--R          +-------+
--R   (3)  x\|a x + b
--R                                                     Type: Expression Integer
--E 3

--S 4 of 25
integrate(t1,x)
--R 
--R
--R           2 2              2  +-------+
--R        (6a x  + 2a b x - 4b )\|a x + b
--R   (4)  --------------------------------
--R                         2
--R                      15a
--R                                          Type: Union(Expression Integer,...)
--E 4

--S 5 of 25
z := sqrt(a**2 - x**2)
--R 
--R
--R         +---------+
--R         |   2    2
--R   (5)  \|- x  + a
--R                                                     Type: Expression Integer
--E 5

--S 6 of 25
t2:=1 / z
--R 
--R
--R              1
--R   (6)  ------------
--R         +---------+
--R         |   2    2
--R        \|- x  + a
--R                                                     Type: Expression Integer
--E 6

--S 7 of 25
integrate(t2,x)
--R 
--R
--R                 +---------+
--R                 |   2    2
--R                \|- x  + a   - a
--R   (7)  - 2atan(----------------)
--R                        x
--R                                          Type: Union(Expression Integer,...)
--E 7

--S 8 of 25
t3:=x**2 * z
--R 
--R
--R           +---------+
--R         2 |   2    2
--R   (8)  x \|- x  + a
--R                                                     Type: Expression Integer
--E 8

--S 9 of 25
integrate(t3,x)
--R 
--R
--R   (9)
--R                           +---------+
--R               5 2      7  |   2    2      4 4      6 2      8
--R         ((- 8a x  + 16a )\|- x  + a   - 2a x  + 16a x  - 16a )
--R      *
--R               +---------+
--R               |   2    2
--R              \|- x  + a   - a
--R         atan(----------------)
--R                      x
--R     + 
--R                                    +---------+
--R        7      2 5      4 3     6   |   2    2        7      3 5      5 3     7
--R     (2x  - 17a x  + 24a x  - 8a x)\|- x  + a   - 8a x  + 28a x  - 28a x  + 8a x
--R  /
--R                     +---------+
--R           2      3  |   2    2      4      2 2      4
--R     (32a x  - 64a )\|- x  + a   + 8x  - 64a x  + 64a
--R                                          Type: Union(Expression Integer,...)
--E 9

--S 10 of 25
t4:=x**3 / (a+b*x)**(1/3)
--R 
--R
--R              3
--R             x
--R   (10)  ----------
--R         3+-------+
--R         \|b x + a
--R                                                     Type: Expression Integer
--E 10

--S 11 of 25
integrate(t4,x)
--R 
--R
--R              3 3         2 2       2          3 3+-------+2
--R         (120b x  - 135a b x  + 162a b x - 243a )\|b x + a
--R   (11)  ---------------------------------------------------
--R                                    4
--R                                440b
--R                                          Type: Union(Expression Integer,...)
--E 11

--S 12 of 25
t5:=1 / (x**3 * (a+b*x)**(1/3))
--R 
--R
--R               1
--R   (12)  ------------
--R          3 3+-------+
--R         x  \|b x + a
--R                                                     Type: Expression Integer
--E 12

--S 13 of 25
integrate(t5,x)
--R 
