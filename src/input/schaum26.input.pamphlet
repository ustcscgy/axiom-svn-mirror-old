\section{\cite{1}:14.527~~~~~$\displaystyle
\int{x^m\ln{x}}~dx$}
$$\int{x^m\ln{x}}=
\frac{x^{m+1}}{m+1}\left(\ln{x}-\frac{1}{m+1}\right)
$$
\begin{chunk}{*}
)clear all

--S 7 of 43
aa:=integrate(x^m*log(x),x)
--R 
--R
--R                               m log(x)
--R        ((m + 1)x log(x) - x)%e
--R   (1)  -------------------------------
--R                   2
--R                  m  + 2m + 1
--R                                          Type: Union(Expression Integer,...)
--E

--S 8 of 43
bb:=x^(m+1)/(m+1)*(log(x)-1/(m+1))
--R
--R                            m + 1
--R        ((m + 1)log(x) - 1)x
--R   (2)  -------------------------
--R                2
--R               m  + 2m + 1
--R                                                     Type: Expression Integer
--E

--S 9 of 43
cc:=aa-bb
--R
--R                               m log(x)                         m + 1
--R        ((m + 1)x log(x) - x)%e         + ((- m - 1)log(x) + 1)x
--R   (3)  -------------------------------------------------------------
--R                                  2
--R                                 m  + 2m + 1
--R                                                     Type: Expression Integer
--E

--S 10 of 43
explog:=rule(%e^(n*log(x)) == x^n)
--R
--R          n log(x)     n
--R   (4)  %e         == x
--R                        Type: RewriteRule(Integer,Integer,Expression Integer)
--E

--S 11 of 43
dd:=explog cc
--R
--R                              m + 1                         m
--R        ((- m - 1)log(x) + 1)x      + ((m + 1)x log(x) - x)x
--R   (5)  -----------------------------------------------------
--R                              2
--R                             m  + 2m + 1
--R                                                     Type: Expression Integer
--E

--S 12 of 43     14:527 Schaums and Axiom agree
ee:=complexNormalize dd
--R
--R   (6)  0
--R                                                     Type: Expression Integer
--E
\end{chunk}

