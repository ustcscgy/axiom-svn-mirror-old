--S 12 of 27
sin(a*t) * cosh(a*t) - cos(a*t) * sinh(a*t)
--R 
--R
--R   (12)  - cos(a t)sinh(a t) + cosh(a t)sin(a t)
--R                                                     Type: Expression Integer
--E 12

--S 13 of 27
laplace(%, t, s)
--R 
--R
--R              3
--R            4a
--R   (13)  --------
--R          4     4
--R         s  + 4a
--R                                                     Type: Expression Integer
--E 13

--S 14 of 27
(exp(a*t) - exp(b*t))/t
--R 
--R
--R             b t     a t
--R         - %e    + %e
