--S 1 of 10
(a*x+b) / (b**2 * x * log(x)**2 + 2*a*b*x**2*log(x) + a**2*x**3 + x)
--R 
--R
--R                        a x + b
--R   (1)  --------------------------------------
--R         2        2         2          2 3
--R        b x log(x)  + 2a b x log(x) + a x  + x
--R                                                     Type: Expression Integer
--E 1

--S 2 of 10
integrate(%,x)
--R 
--R
--R   (2)  atan(b log(x) + a x)
--R                                          Type: Union(Expression Integer,...)
--E 2

--S 3 of 10
((exp(x)-x**2+2*x)/(x**2*(exp(x)+x)**2))*exp((x**2-1)/x+1/(exp(x)+x))
--R 
--R
--R                           2       x    3
--R                         (x  - 1)%e  + x
--R                         ----------------
--R                                x    2
--R           x    2           x %e  + x
--R        (%e  - x  + 2x)%e
--R   (3)  ---------------------------------
--R               2   x 2     3  x    4
--R              x (%e )  + 2x %e  + x
--R                                                     Type: Expression Integer
--E 3

--S 4 of 10
integrate(%,x)
--R 
--R
--R            2       x    3
--R          (x  - 1)%e  + x
--R          ----------------
--R                 x    2
--R             x %e  + x
--R        %e
--R   (4)  ------------------
--R                  x
--R                %e
--R                                          Type: Union(Expression Integer,...)
--E 4

