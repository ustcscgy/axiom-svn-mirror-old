--R
--R            x - a
--R        log(-----)
--R            x + a
--R   (2)  ----------
--R            2a
--R                                                     Type: Expression Integer
--E

--S 3
cc:=aa-bb
--R
--R                                        x - a
--R        - log(x + a) + log(x - a) - log(-----)
--R                                        x + a
--R   (3)  --------------------------------------
--R                          2a
--R                                                     Type: Expression Integer
--E

--S 4
divlog:=rule(log(a/b) == log(a) - log(b))
--R
--R            a
--R   (4)  log(-) == - log(b) + log(a)
--R            b
--R                        Type: RewriteRule(Integer,Integer,Expression Integer)
--E

--S 5      14:144 Schaums and Axiom agree
dd:=divlog cc
--R
--R   (5)  0
--R                                                     Type: Expression Integer
--E

