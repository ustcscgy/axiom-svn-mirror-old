rm -f /tmp/done && make clean && make NOISE= && rm -f allnew.md5 && for i in `find . -type f | egrep -v '(\.dvi|\.a|gcl-2\.6|\.fn|\.daase|\.output)'` ; do md5sum $i >>allnew.md5 ; done && diff -Naur src/regress/all.md5 allnew.md5 | grep "+" | grep -v "@" | tee /tmp/done 
if [ -f /tmp/done ] ; then echo `date` >>/tmp/done && scp /tmp/done daly@axiom-developer.org:/home/daly/done && ssh daly@axiom-developer.org 'cat done | mail -s "done" daly@cert.org' ; else ssh daly@axiom-developer.org 'echo failed | mail -s "failed" daly@cert.org' ; fi

