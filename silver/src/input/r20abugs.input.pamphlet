@
This file tests bugs fixed/additions made since release 2.0a.
Created by Mike Dewar, 17.5.95

-- Added eval for aggregates
<<*>>=
--S 1 of 34
m : Matrix Expression Integer := matrix [[i*x^j for i in 1..3] for j in 1..3]
--R 
--R
--R        +x   2x   3x +
--R        |            |
--R        | 2    2    2|
--R   (1)  |x   2x   3x |
--R        |            |
--R        | 3    3    3|
--R        +x   2x   3x +
--R                                              Type: Matrix Expression Integer
--E 1

--S 2 of 34
eval(m,x=0)
--R 
--R
--R        +0  0  0+
--R        |       |
--R   (2)  |0  0  0|
--R        |       |
--R        +0  0  0+
--R                                              Type: Matrix Expression Integer
--E 2

@
Added seed function
<<*>>=

)clear completely
--S 3 of 34
s:= seed();
--R 
--R
--R                                                        Type: PositiveInteger
--E 3

--S 4 of 34
r1 := randnum();
--R 
--R
--R                                                        Type: PositiveInteger
--E 4

--S 5 of 34
for i in 1..10 repeat randnum();
--R 
--R                                                                   Type: Void
--E 5

--S 6 of 34
reseed s;
--R 
--R                                                                   Type: Void
--E 6

--S 7 of 34
r2 := randnum();
--R 
--R
--R                                                        Type: PositiveInteger
--E 7

--S 8 of 34
r1 - r2
--R 
--R
--R   (6)  0
--R                                                     Type: NonNegativeInteger
--E 8

@
Fixed some problems with \%e etc in rules
<<*>>=

)clear completely

--S 9 of 34
r3:=rule(3==%pi) -- biblical approximation
--R 
--R
--R   (1)  3 == %pi
--R                        Type: RewriteRule(Integer,Integer,Expression Integer)
--E 9

--S 10 of 34
numeric(%pi)
--R 
--R
--R   (2)  3.1415926535 897932385
--R                                                                  Type: Float
--E 10

--S 11 of 34
numeric(r3(3))
--R 
--R
--R   (3)  3.1415926535 897932385
--R                                                                  Type: Float
--E 11

)clear completely

@
Added some extra simplifications
<<*>>=

--S 12 of 34
sin(atan(sqrt 3)/2)
--R 
--R
--R        1
--R   (1)  -
--R        2
--R                                                     Type: Expression Integer
--E 12

)clear completely
 
@
Fix to poly from Quitte
<<*>>=
--S 13 of 34
R := IntegerMod(4)
--R 
--R
--R   (1)  IntegerMod 4
--R                                                                 Type: Domain
--E 13

--S 14 of 34
PolR := UP('X, R)
--R 
--R
--R   (2)  UnivariatePolynomial(X,IntegerMod 4)
--R                                                                 Type: Domain
--E 14

--S 15 of 34
X : PolR := monomial(1, 1)
--R 
--R
--R   (3)  X
--R                                   Type: UnivariatePolynomial(X,IntegerMod 4)
--E 15

--S 16 of 34
a : PolR := 2 * X**2
--R 
--R
--R          2
--R   (4)  2X
--R                                   Type: UnivariatePolynomial(X,IntegerMod 4)
--E 16

--S 17 of 34
b : PolR := X**2 + 2*X + 1
--R 
--R
--R         2
--R   (5)  X  + 2X + 1
--R                                   Type: UnivariatePolynomial(X,IntegerMod 4)
--E 17

@
Used to give a 0 remainder!
<<*>>=
--S 18 of 34
qr := monicDivide(a, b)
--R 
--R
--R   (6)  [quotient= 2,remainder= 2]
--RType: Record(quotient: UnivariatePolynomial(X,IntegerMod 4),remainder: UnivariatePolynomial(X,IntegerMod 4))
--E 18

--S 19 of 34
a - (qr.quotient * b + qr.remainder)
--R 
--R
--R   (7)  0
--R                                   Type: UnivariatePolynomial(X,IntegerMod 4)
--E 19

)clear completely

@
This used not to return 0
<<*>>=
--S 20 of 34
limit(%e^(1/x^2)/(%e^(1/x^2) + %e^(1/x^4)), x=0)
--R 
